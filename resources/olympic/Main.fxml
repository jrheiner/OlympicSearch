<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0"
         tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/10.0.2-internal"
         fx:controller="olympic.ui.MainController">
    <Tab text="Athletes">
        <fx:include source="tabs/AthleteTab.fxml" fx:id="AthleteTab"/>
    </Tab>
    <Tab text="Teams">
        <fx:include source="tabs/TeamTab.fxml" fx:id="TeamTab"/>
    </Tab>
    <Tab text="Events">
        <fx:include source="tabs/EventTab.fxml" fx:id="EventTab"/>
    </Tab>
    <Tab text="Disciplines">
        <fx:include source="tabs/DisciplineTab.fxml" fx:id="DisciplineTab"/>
    </Tab>

    <Tab text="Olympic Games">
        <fx:include source="tabs/OlympicGameTab.fxml" fx:id="OlympicGameTab"/>
    </Tab>
</TabPane>
